{
  "name": "cuhk-course-selection-backend",
  "version": "1.0.0",
  "description": "Course Selection System Backend - Database Class Project",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "npx prisma generate && tsc",
    "start": "node dist/server.js",
    "worker": "ts-node-dev --respawn --transpile-only src/workers/enrollmentWorker.ts",
    "migrate": "ts-node database/migrate.ts",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:seed": "ts-node ../prisma/seed.ts",
    "prisma:studio": "npx prisma studio",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed",
    "queue:clear": "ts-node src/scripts/clearStuckJobs.ts"
  },
  "prisma": {
    "seed": "ts-node ../prisma/seed.ts"
  },
  "keywords": [
    "course-selection",
    "typescript",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "prisma": "^6.19.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
